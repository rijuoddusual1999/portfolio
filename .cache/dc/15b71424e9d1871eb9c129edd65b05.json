r\n\r\n.svg_box{\r\n    position: absolute;\r\n    width: 100%;\r\n    height: 100vh;\r\n    overflow: hidden;\r\n    background: url('../images/diane-picchiottino-o9o1ihCnEMQ-unsplash.jpg');\r\n    background-size: cover;\r\n    \r\n    \r\n}\r\n\r\n.box{\r\n    position: absolute;\r\n    background: url('../images/ai-generated-7975718_1920.png');\r\n    background-attachment: fixed;\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n.fancy_button {\r\n    width: 6em;\r\n    height: 1.9em;\r\n    border: none;\r\n    text-decoration: none;\r\n    font-family: 'Roboto Condensed', sans-serif;\r\n    font-weight: 900;\r\n    font-size: 1rem ;\r\n    color: rgb(105, 0, 0);\r\n    background-color: rgb(252, 89, 29);\r\n    border-radius: 3.75em;\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    box-shadow: 0 0 0.8em rgba(252, 89, 29, 1), 0 0 1.3em rgba(252, 89, 29, 1);\r\n    transition: 0.5s;\r\n    letter-spacing: 0.2em;\r\n}\r\n\r\n.fancy_button:hover {\r\n    transform: scale(0.9);\r\n    animation: pulse 0.7s linear infinite;\r\n}\r\n\r\n@keyframes pulse {\r\n    0% {\r\n        box-shadow: 0 0 0.8em rgba(252, 89, 29, 1), 0 0 1.3em rgba(252, 89, 29, 1);\r\n    }\r\n    50% {\r\n        box-shadow: 0 0 0 0.5em rgba(243, 20, 8, 0.545), 0 0 0 1.3em rgba(252, 89, 29, 0.5);\r\n        filter: blur(0.05em);\r\n    }\r\n    100% {\r\n        box-shadow: 0 0 0.9em rgba(251, 104, 51, 0.8), 0 0 1.8em rgba(252, 89, 29, 1);\r\n    }\r\n}\r\n\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"0e275c20ec9ddecd0d4819962553064e","cacheData":{"env":{}}}